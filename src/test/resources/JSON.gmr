//General
LINE_END_WIN='\r\n'
LINE_END_UNIX='\n'
LINE_END_OSX='\r'
CHRACTERS='(?<!\\)[A-Za-z]+'
UNDERSCORE='_'
EQUAL='[=]'
WHITESPACE='[ \t]+'
COMMA=','
COLON=':'
POINT='\.'
EOF='\Z'


//Unescaped
QUOTE='(?<!\\)"'
BACKSLASH='\\(?!\')'
SLASH='(?<!\\)\/'
BACKSPACE='\b'
FORMFEED='\f'
TAB='\t'

ESCAPED_QUOTE='\\\''
ESCAPED_BACKSLASH='\\\\'
ESCAPED_SLASH='\\\/'
ESCAPED_BACKSPACE='\\b'
ESCAPED_FORMFEED='\\f'
ESCAPED_TAB='\\t'


//Specific
TOKEN_SIGN='#'
OPTIONAL='\?'
STAR='\*'
ARROW='>'
ARRAY_OPEN='\{'
ARRAY_CLOSE='\}'
IGNORE='~'